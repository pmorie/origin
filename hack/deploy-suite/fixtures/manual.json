{
    "id":"hello-deployment-config",
    "kind":"DeploymentConfig",
    "apiVersion":"v1beta1",
    "triggers":[
        {
            "type": "manual"
        },
        {
            "type": "image-change",
            "auto": true,
            "imageChangeParams": {
                "containerNames":[
                    "hello-openshift"
                ],
                "repositoryName": "localhost:5000/openshift/hello-openshift",
                "tag": "latest"
            }
        },
        {
            "type": "config-change",
            "auto": true,
        }
    ],
    "template":{
        "strategy":{
            "type":"customPod",
            "customPod":{
                "image":"localhost:5000/openshift/kube-deploy"
            }
        },
        "controllerTemplate":{
            "replicas":1,
            "replicaSelector":{
                "name":"hello-openshift"
            },
            "podTemplate":{
                "desiredState":{
                    "manifest":{
                        "version":"v1beta1",
                        "id":"hello-openshift",
                        "containers":[
                            {
                                "name":"hello-openshift",
                                "image":"openshift/hello-openshift",
                                "env":[
                                    {
                                        "name": "HELLO_ENV1",
                                        "value": "HELLO_ENV1_VALUE"
                                    }
                                ],
                                "ports":[
                                    {
                                        "containerPort":8080
                                    }
                                ]
                            }
                        ]
                    }
                },
                "labels":{
                    "name":"hello-openshift"
                }
            }
        }
    }
}
